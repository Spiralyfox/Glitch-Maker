{
  "tags": [
    "Robot",
    "Lo-fi",
    "Dark",
    "Nightcore",
    "Experimental",
    "Clean"
  ],
  "presets": [
    {
      "name": "Robot Voice",
      "description": "Ring modulation + bitcrush — metallic robotic tone",
      "tags": [
        "Robot",
        "Experimental"
      ],
      "effects": [
        {
          "name": "ring_mod",
          "params": {
            "freq": 200,
            "mix": 0.6
          }
        },
        {
          "name": "bitcrusher",
          "params": {
            "bits": 8,
            "downsample": 2
          }
        }
      ],
      "builtin": true
    },
    {
      "name": "Vocoder Robot",
      "description": "Robotic resynthesis — small grains, monotone, metallic",
      "tags": [
        "Robot"
      ],
      "effects": [
        {
          "name": "robot",
          "params": {
            "grain_ms": 15,
            "monotone": true,
            "formant": 1.0,
            "mix": 0.8
          }
        }
      ],
      "builtin": true
    },
    {
      "name": "Chipmunk",
      "description": "Pitch up +7 — high-pitched playful voice",
      "tags": [
        "Nightcore"
      ],
      "effects": [
        {
          "name": "pitch_shift",
          "params": {
            "semitones": 7
          }
        }
      ],
      "builtin": true
    },
    {
      "name": "Demon Voice",
      "description": "Pitch down -12 + distortion — deep menacing vocal",
      "tags": [
        "Dark"
      ],
      "effects": [
        {
          "name": "pitch_shift",
          "params": {
            "semitones": -12
          }
        },
        {
          "name": "distortion",
          "params": {
            "type": "fuzz",
            "drive": 4.0,
            "mix": 0.3
          }
        }
      ],
      "builtin": true
    },
    {
      "name": "Witch House",
      "description": "Pitch down -5 + heavy reverb + slow phaser — dark, eerie",
      "tags": [
        "Dark"
      ],
      "effects": [
        {
          "name": "pitch_shift",
          "params": {
            "semitones": -5
          }
        },
        {
          "name": "delay",
          "params": {
            "delay_ms": 400,
            "feedback": 0.6,
            "mix": 0.5
          }
        },
        {
          "name": "phaser",
          "params": {
            "rate": 0.3,
            "depth": 0.7,
            "mix": 0.4
          }
        }
      ],
      "builtin": true
    },
    {
      "name": "Lo-fi Vocal",
      "description": "Lowpass filter + vinyl texture — warm nostalgic",
      "tags": [
        "Lo-fi"
      ],
      "effects": [
        {
          "name": "filter",
          "params": {
            "type": "lowpass",
            "cutoff": 3000,
            "resonance": 0.3
          }
        },
        {
          "name": "vinyl",
          "params": {
            "crackle": 0.4,
            "hiss": 0.2,
            "wow": 0.15
          }
        }
      ],
      "builtin": true
    },
    {
      "name": "Phonk Vocal",
