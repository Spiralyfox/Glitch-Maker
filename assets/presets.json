      "tags": [
        "Experimental"
      ],
      "effects": [
        {
          "name": "ring_mod",
          "params": {
            "freq": 350,
            "mix": 0.5
          }
        },
        {
          "name": "chorus",
          "params": {
            "rate": 1.2,
            "depth": 0.5,
            "mix": 0.4
          }
        },
        {
          "name": "delay",
          "params": {
            "delay_ms": 200,
            "feedback": 0.4,
            "mix": 0.3
          }
        }
      ],
      "builtin": true
    },
    {
      "name": "Broken Radio",
      "description": "Bandpass filter + bitcrush + distortion — lo-fi radio",
      "tags": [
        "Lo-fi",
        "Experimental"
      ],
      "effects": [
        {
          "name": "filter",
          "params": {
            "type": "bandpass",
            "cutoff": 2000,
            "resonance": 0.6
          }
        },
        {
          "name": "bitcrusher",
          "params": {
            "bits": 8,
            "downsample": 3
          }
        },
        {
          "name": "distortion",
          "params": {
