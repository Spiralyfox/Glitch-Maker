      "tags": [
        "Lo-fi"
      ],
      "effects": [
        {
          "name": "pitch_shift",
          "params": {
            "semitones": -3
          }
        },
        {
          "name": "delay",
          "params": {
            "delay_ms": 350,
            "feedback": 0.5,
            "mix": 0.45
          }
        },
        {
          "name": "chorus",
          "params": {
            "rate": 0.5,
            "depth": 0.3,
            "mix": 0.3
          }
        }
      ],
      "builtin": true
    }
  ]
