      "tags": [
        "Dark",
        "Experimental"
      ],
      "effects": [
        {
          "name": "distortion",
          "params": {
            "type": "tube",
            "drive": 5.0,
            "mix": 0.5
          }
        },
        {
          "name": "filter",
          "params": {
            "type": "highpass",
            "cutoff": 800,
            "resonance": 0.4
          }
        },
        {
          "name": "delay",
          "params": {
            "delay_ms": 120,
            "feedback": 0.3,
            "mix": 0.2
          }
        }
      ],
      "builtin": true
    },
    {
      "name": "Alien Voice",
